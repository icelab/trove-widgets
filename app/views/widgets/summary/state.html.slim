.summary style="height:#{params[:height].to_i-74}px;"

  h1.summary__title #{@state} state newspapers
  p.summary__stats.summary__stats--state #{human_counts('title', @issuecount)} from #{@start_date} to #{@end_date}

  .search.search= render 'form', placeholder: 'Search across state titles', hidden_name: 'l-state', hidden_value: @state
